name: digit-recognition

docker_env:
    image: mnist-dockerized

entry_points:
  main:
    parameters:
      vm-flavor: string,
      github-uri: string,
      path-to-data: {type: string, default: 'data/'}
    command: "python train.py --path-to-data {path-to-data} --vm-flavor {vm-flavor} --github-uri {github-uri}"

